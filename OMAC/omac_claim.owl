<?xml version="1.0"?>
<rdf:RDF xmlns="http://www.omac.org/claim#"
     xml:base="http://www.omac.org/claim"
     xmlns:oc="http://www.omac.org/claim#"
     xmlns:od="http://www.omac.org/data#"
     xmlns:ow="http://www.omac.org/work#"
     xmlns:dbo="http://dbpedia.org/ontology/"
     xmlns:dbp="http://dbpedia.org/property/"
     xmlns:owl="http://www.w3.org/2002/07/owl#"
     xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
     xmlns:xml="http://www.w3.org/XML/1998/namespace"
     xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
     xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
     xmlns:skos="http://www.w3.org/2004/02/skos/core#"
     xmlns:viaf="http://viaf.org/viaf/"
     xmlns:terms="http://purl.org/dc/terms/">
    <owl:Ontology rdf:about="http://www.omac.org/claim">
        <owl:imports rdf:resource="http://www.omac.org/work"/>
        <terms:creator rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Emilio M. Sanfilippo, Laboratory for Applied Ontology, ISTC-CNR
Richard Freedman, Haverford College, USA</terms:creator>
        <terms:subject rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Scholarly claims about musical entities</terms:subject>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Data present in this file are meant for testing purposes only and will be removed in the final version of the ontology</rdfs:comment>
        <owl:versionInfo>under development</owl:versionInfo>
    </owl:Ontology>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Object Properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://www.omac.org/claim#analyzed_by -->

    <owl:ObjectProperty rdf:about="http://www.omac.org/claim#analyzed_by">
        <owl:inverseOf rdf:resource="http://www.omac.org/claim#analyzes"/>
        <rdfs:domain rdf:resource="http://www.omac.org/work#MusicalEntity"/>
        <rdfs:range rdf:resource="http://dbpedia.org/ontology/Person"/>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">analyzed by</rdfs:label>
        <skos:prefLabel rdf:datatype="http://www.w3.org/2001/XMLSchema#string">analyzed by</skos:prefLabel>
    </owl:ObjectProperty>
    


    <!-- http://www.omac.org/claim#analyzes -->

    <owl:ObjectProperty rdf:about="http://www.omac.org/claim#analyzes">
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">analyzes</rdfs:label>
        <skos:prefLabel rdf:datatype="http://www.w3.org/2001/XMLSchema#string">analyzes</skos:prefLabel>
    </owl:ObjectProperty>
    


    <!-- http://www.omac.org/claim#attributedAuthor -->

    <owl:ObjectProperty rdf:about="http://www.omac.org/claim#attributedAuthor">
        <rdfs:comment>relation for testing purposes wrt AuthorshipClaim</rdfs:comment>
        <owl:versionInfo rdf:datatype="http://www.w3.org/2001/XMLSchema#string">TESTING</owl:versionInfo>
    </owl:ObjectProperty>
    


    <!-- http://www.omac.org/claim#derivative_in -->

    <owl:ObjectProperty rdf:about="http://www.omac.org/claim#derivative_in">
        <owl:inverseOf rdf:resource="http://www.omac.org/claim#has_derivative"/>
        <rdfs:domain rdf:resource="http://www.omac.org/work#MusicalEntity"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Included in OMAC claim module</rdfs:comment>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Used for improving automated reasoning
(inverse of has_derivative)</rdfs:comment>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">derivative in</rdfs:label>
        <skos:prefLabel xml:lang="en">derivative in</skos:prefLabel>
    </owl:ObjectProperty>
    


    <!-- http://www.omac.org/claim#derivative_of -->

    <owl:ObjectProperty rdf:about="http://www.omac.org/claim#derivative_of">
        <owl:inverseOf rdf:resource="http://www.omac.org/claim#model_for"/>
        <rdfs:domain rdf:resource="http://www.omac.org/work#MusicalEntity"/>
        <rdfs:range rdf:resource="http://www.omac.org/work#MusicalEntity"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Included in OMAC claim module</rdfs:comment>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Relation used to model a work that derivates from another work</rdfs:comment>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">derivative of</rdfs:label>
        <skos:prefLabel xml:lang="en">derivative of</skos:prefLabel>
    </owl:ObjectProperty>
    


    <!-- http://www.omac.org/claim#has_analytic_segment -->

    <owl:ObjectProperty rdf:about="http://www.omac.org/claim#has_analytic_segment">
        <rdfs:subPropertyOf rdf:resource="http://www.omac.org/work#has_part"/>
        <owl:inverseOf rdf:resource="http://www.omac.org/claim#segment_of"/>
        <rdfs:domain>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://www.omac.org/work#MusicalWork"/>
                    <rdf:Description rdf:about="http://www.omac.org/work#MusicalWorkPart"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:domain>
        <rdfs:range rdf:resource="http://www.omac.org/work#MusicalEntity"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Included in OMAC claim module</rdfs:comment>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">has analytic segment</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.omac.org/claim#has_derivative -->

    <owl:ObjectProperty rdf:about="http://www.omac.org/claim#has_derivative">
        <rdfs:subPropertyOf rdf:resource="http://www.omac.org/claim#refers_to"/>
        <rdfs:domain rdf:resource="http://www.omac.org/claim#SimilarityClaim"/>
        <rdfs:range rdf:resource="http://www.omac.org/work#MusicalEntity"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Included in OMAC claim module</rdfs:comment>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">has derivative</rdfs:label>
        <skos:prefLabel xml:lang="en">has derivative</skos:prefLabel>
    </owl:ObjectProperty>
    


    <!-- http://www.omac.org/claim#has_model -->

    <owl:ObjectProperty rdf:about="http://www.omac.org/claim#has_model">
        <rdfs:subPropertyOf rdf:resource="http://www.omac.org/claim#refers_to"/>
        <owl:inverseOf rdf:resource="http://www.omac.org/claim#model_in"/>
        <rdfs:domain rdf:resource="http://www.omac.org/claim#SimilarityClaim"/>
        <rdfs:range rdf:resource="http://www.omac.org/work#MusicalEntity"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Included in OMAC claim module</rdfs:comment>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">has model</rdfs:label>
        <skos:prefLabel xml:lang="en">has model</skos:prefLabel>
    </owl:ObjectProperty>
    


    <!-- http://www.omac.org/claim#has_schema -->

    <owl:ObjectProperty rdf:about="http://www.omac.org/claim#has_schema">
        <rdfs:range>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://www.omac.org/claim#SchemaAttribute"/>
                    <rdf:Description rdf:about="http://www.omac.org/claim#SchemaValue"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:range>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Included in OMAC claim module</rdfs:comment>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">has schema</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.omac.org/claim#model_for -->

    <owl:ObjectProperty rdf:about="http://www.omac.org/claim#model_for">
        <rdfs:domain rdf:resource="http://www.omac.org/work#MusicalEntity"/>
        <rdfs:range rdf:resource="http://www.omac.org/work#MusicalEntity"/>
        <owl:propertyChainAxiom rdf:parseType="Collection">
            <rdf:Description rdf:about="http://www.omac.org/claim#model_in"/>
            <rdf:Description rdf:about="http://www.omac.org/claim#has_derivative"/>
        </owl:propertyChainAxiom>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Included in OMAC claim module</rdfs:comment>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Relation used to represent a musical work that is used as model for the composition of another musical work (by the same or different composer)</rdfs:comment>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">model for</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.omac.org/claim#model_in -->

    <owl:ObjectProperty rdf:about="http://www.omac.org/claim#model_in">
        <rdfs:domain rdf:resource="http://www.omac.org/work#MusicalEntity"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Included in OMAC claim module</rdfs:comment>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Used for improving automated reasoning
(inverse of has_model)</rdfs:comment>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">model in</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.omac.org/claim#refers_to -->

    <owl:ObjectProperty rdf:about="http://www.omac.org/claim#refers_to">
        <owl:inverseOf rdf:resource="http://www.omac.org/claim#related_to"/>
        <rdfs:domain rdf:resource="http://www.omac.org/claim#Claim"/>
        <rdfs:range rdf:resource="http://www.w3.org/2002/07/owl#Thing"/>
        <owl:propertyChainAxiom rdf:parseType="Collection">
            <rdf:Description rdf:about="http://www.omac.org/claim#refers_to"/>
            <rdf:Description rdf:about="http://www.omac.org/work#part_of"/>
        </owl:propertyChainAxiom>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Included in OMAC claim module</rdfs:comment>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">refers to</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.omac.org/claim#related_to -->

    <owl:ObjectProperty rdf:about="http://www.omac.org/claim#related_to">
        <rdfs:domain rdf:resource="http://www.w3.org/2002/07/owl#Thing"/>
        <rdfs:range rdf:resource="http://www.omac.org/claim#Claim"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Included in OMAC claim module</rdfs:comment>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">related to</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.omac.org/claim#segment_of -->

    <owl:ObjectProperty rdf:about="http://www.omac.org/claim#segment_of">
        <rdfs:subPropertyOf rdf:resource="http://www.omac.org/work#part_of"/>
        <rdfs:domain rdf:resource="http://www.omac.org/work#MusicalEntity"/>
        <rdfs:range>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://www.omac.org/work#MusicalWork"/>
                    <rdf:Description rdf:about="http://www.omac.org/work#MusicalWorkPart"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:range>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">analytic segment of</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.omac.org/claim#stated_by -->

    <owl:ObjectProperty rdf:about="http://www.omac.org/claim#stated_by">
        <owl:inverseOf rdf:resource="http://www.omac.org/claim#states"/>
        <rdfs:domain rdf:resource="http://www.omac.org/claim#Claim"/>
        <rdfs:range rdf:resource="http://dbpedia.org/ontology/Person"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Included in OMAC claim module</rdfs:comment>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">stated by</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.omac.org/claim#states -->

    <owl:ObjectProperty rdf:about="http://www.omac.org/claim#states">
