    <!-- http://dbpedia.org/ontology/Person -->

    <rdf:Description rdf:about="http://dbpedia.org/ontology/Person">
        <owl:disjointWith rdf:resource="http://www.omac.org/claim#Claim"/>
    </rdf:Description>
    


    <!-- http://dbpedia.org/ontology/Place -->

    <rdf:Description rdf:about="http://dbpedia.org/ontology/Place">
        <owl:disjointWith rdf:resource="http://www.omac.org/claim#Claim"/>
    </rdf:Description>
    


    <!-- http://www.omac.org/claim#Analyst -->

    <owl:Class rdf:about="http://www.omac.org/claim#Analyst">
        <owl:equivalentClass>
            <owl:Class>
                <owl:intersectionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://dbpedia.org/ontology/Person"/>
                    <owl:Class>
                        <owl:unionOf rdf:parseType="Collection">
                            <owl:Restriction>
                                <owl:onProperty rdf:resource="http://www.omac.org/claim#analyzes"/>
                                <owl:someValuesFrom rdf:resource="http://www.omac.org/work#MusicalEntity"/>
                            </owl:Restriction>
                            <owl:Restriction>
                                <owl:onProperty rdf:resource="http://www.omac.org/claim#states"/>
                                <owl:someValuesFrom rdf:resource="http://www.omac.org/claim#Claim"/>
                            </owl:Restriction>
                        </owl:unionOf>
                    </owl:Class>
                </owl:intersectionOf>
            </owl:Class>
        </owl:equivalentClass>
    </owl:Class>
    


    <!-- http://www.omac.org/claim#AnalyticSegment -->

    <owl:Class rdf:about="http://www.omac.org/claim#AnalyticSegment">
        <owl:equivalentClass>
            <owl:Class>
                <owl:intersectionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://www.omac.org/work#MusicalEntity"/>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="http://www.omac.org/claim#segment_of"/>
                        <owl:someValuesFrom>
                            <owl:Class>
                                <owl:unionOf rdf:parseType="Collection">
                                    <rdf:Description rdf:about="http://www.omac.org/work#MusicalWork"/>
                                    <rdf:Description rdf:about="http://www.omac.org/work#MusicalWorkPart"/>
                                </owl:unionOf>
                            </owl:Class>
                        </owl:someValuesFrom>
                    </owl:Restriction>
                </owl:intersectionOf>
            </owl:Class>
        </owl:equivalentClass>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.omac.org/claim#analyzed_by"/>
                <owl:someValuesFrom rdf:resource="http://dbpedia.org/ontology/Person"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">An analytic segment is a pattern of a musical work or musical work part identified by an analyst. It is not therefore an authorial part of a musical work</rdfs:comment>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Analytic Segment</rdfs:label>
        <skos:prefLabel xml:lang="en">Analytic Segment</skos:prefLabel>
    </owl:Class>
    


    <!-- http://www.omac.org/claim#AuthorshipClaim -->

    <owl:Class rdf:about="http://www.omac.org/claim#AuthorshipClaim">
        <rdfs:subClassOf rdf:resource="http://www.omac.org/claim#Claim"/>
        <rdfs:comment>This class can be used to represent a claim about authorship, e.g., to claim that a certain person is meant (by a scholar) to be the composer of a musical work</rdfs:comment>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Authorship Claim</rdfs:label>
        <owl:versionInfo xml:lang="en">TESTING</owl:versionInfo>
        <skos:prefLabel xml:lang="en">Authorship Claim</skos:prefLabel>
    </owl:Class>
    


    <!-- http://www.omac.org/claim#Claim -->

    <owl:Class rdf:about="http://www.omac.org/claim#Claim">
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.omac.org/claim#refers_to"/>
                <owl:someValuesFrom>
                    <owl:Class>
                        <owl:unionOf rdf:parseType="Collection">
                            <rdf:Description rdf:about="http://dbpedia.org/ontology/Person"/>
                            <rdf:Description rdf:about="http://www.omac.org/work#MusicalEntity"/>
                        </owl:unionOf>
                    </owl:Class>
                </owl:someValuesFrom>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.omac.org/claim#stated_by"/>
                <owl:someValuesFrom rdf:resource="http://dbpedia.org/ontology/Person"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <owl:disjointWith rdf:resource="http://www.omac.org/work#Attribute"/>
        <owl:disjointWith rdf:resource="http://www.omac.org/work#MusicalEntity"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A claim is a scholarly observation (statement) of a musical phenomenon according to which the latter is characterised by a scholar in terms of various properties</rdfs:comment>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Included in OMAC claim module</rdfs:comment>
        <rdfs:comment xml:lang="en">The list of claims hereby included is not exhaustive</rdfs:comment>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Claim</rdfs:label>
    </owl:Class>
    <owl:Axiom>
        <owl:annotatedSource rdf:resource="http://www.omac.org/claim#Claim"/>
        <owl:annotatedProperty rdf:resource="http://www.w3.org/2000/01/rdf-schema#subClassOf"/>
        <owl:annotatedTarget>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.omac.org/claim#refers_to"/>
                <owl:someValuesFrom>
                    <owl:Class>
                        <owl:unionOf rdf:parseType="Collection">
                            <rdf:Description rdf:about="http://dbpedia.org/ontology/Person"/>
                            <rdf:Description rdf:about="http://www.omac.org/work#MusicalEntity"/>
                        </owl:unionOf>
                    </owl:Class>
                </owl:someValuesFrom>
            </owl:Restriction>
        </owl:annotatedTarget>
        <rdfs:comment>This axioms provides a first characterization of the Claim class; it needs to be refined according to more precise application requirements</rdfs:comment>
    </owl:Axiom>
    


